{
    "manifest_version": 2,
    "name": "Hermes",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "commands": {
        "toggle-feature-foo": {
          "suggested_key": {
            "default": "Alt+Q",
            "mac": "Alt+Q"
          },
          "description": "Toggle feature foo"
        }
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    }
}
